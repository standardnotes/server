    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/newrelic": "^7.0.3",
    "@types/prettyjson": "^0.0.30",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.2",
    "nodemon": "^2.0.19",
    "npm-check-updates": "^16.0.1",
